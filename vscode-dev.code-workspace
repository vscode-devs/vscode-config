{
	"folders": [
		{
			"path": "."
		}
	],
	"settings": {
		// 搜索时不想显示的文件可按以下格式屏蔽(为 true 时屏蔽)
		"search.exclude": {
            "**/*.o":true,
            "**/*.su":true,
            "**/*.cmd":true,
		},
		// 不想在工作区显示的文件可按以下格式屏蔽(为 true 时屏蔽)
		"files.exclude": {
			"**/*.o":true,
            "**/*.su":true,
            "**/*.cmd":true,
		},
	}
}